  lcmarginscy,          -- 开证保证金人民币金额\n    paymode,               -- 收支方式\n    paymodetype,           -- 收款方式\n    abgoods,               -- 货前后\n    lcmarginrcy1,         -- 开证保证金人民币金额2\n    lcmarginrcy2,         -- 开证保证金人民币金额3\n    odate,                 -- 日期\n    cuicode,               -- 商户\n    lcappicode,            -- 收到信用申请单内码\n    lccode,                -- 信用证号码\n    lcappcode,             -- 登记流水号\n    adate,                 -- 实际收款日期\n    rldate,                -- 预计收证日期\n    aldate,                -- 实际收证日期\n    afcy,                  -- 实际到款原币金额\n    ascy,                  -- 实际到款折本位币金额\n    arcy1,                 -- 实际到款折报告币1金额\n    arcy2,                 -- 实际到款折报告币2金额\n    idx,                   -- 序号\n    prjicode,              -- 项目内码外键\n    prjcode,               -- 项目号\n    paytype,               -- 支付方式\n    nx_icode,              -- NX系统主键\n    nx_idx,                -- NX次序\n    pticode,               -- 协议内码\n    pticodex,              -- 协议核销内码\n    u_jstype,              -- 结算方式2\n    CURRENT_DATE()           -- 当前日期\nFROM \n    dwi_sal.dwi_sal_purordersettlement_n9 -- 表名\n\n        LIMIT 10; -- 表名\n",
